{
  "name": "ug-react",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint"
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/ug-react.es.js",
      "require": "./dist/ug-react.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/ug-react.umd.js",
  "module": "./dist/ug-react.es.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.2.2",
    "react-hooks": "^1.0.1",
    "sass": "^1.69.7",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-linter": "^2.0.7",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dev-umang/ug-react.git"
  },
  "keywords": [
    "React Components",
    "React Library"
  ],
  "author": {
    "email": "dev.umangjoshi@gmail.com",
    "name": "Umang Joshi"
  },
  "bugs": {
    "url": "git@github.com:dev-umang/ug-react.git/issues"
  },
  "homepage": "git@github.com:dev-umang/ug-react.git#readme",
  "license": "MIT"
}
